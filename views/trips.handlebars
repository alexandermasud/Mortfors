<!-- Modal -->
<div class="modal fade" id="formModal" tabindex="-1" role="dialog" aria-labelledby="formModalLabel">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
        <form method="post" action="/createTrip">
      <div class="modal-header">
          <h4 class="modal-title" id="myModalLabel">Skapa ny resa</h4>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        
      </div>
            
      <div class="modal-body">

          <div class="container">
   
        <div class="row">
<div class="col-lg-6">
  <div class="form-group" >
    <label for="avgangsland">Avgångsland</label>
    <input type="text" class="form-control" name="avgangsland" placeholder="ex Sverige" required>
  </div>
      </div>
          
         <div class="col-lg-6">
  <div class="form-group" >
    <label for="avgangsstad">Avgångsstad</label>
    <input type="text" class="form-control" name="avgangsstad" placeholder="ex Malmö" required>
  </div>
      </div>
            
        </div>
         <div class="row">
            
      <div class="col-lg-6">
  <div class="form-group" >
    <label for="ankomstland">Ankomstland</label>
    <input type="text" class="form-control" name="ankomstland" placeholder="ex Spanien" required>
  </div>
      </div>
           <div class="col-lg-6">
  <div class="form-group" >
    <label for="ankomststad">Ankomststad</label>
    <input type="text" class="form-control" name="ankomststad" placeholder="ex Madrid" required>
  </div>
      </div>
  
            </div>
              <div class="row">
        <div class="col-lg-6">
  <div class="form-group" >
    <label for="avgang">Avgång</label>
    <input type="text" class="form-control" name="avgang" placeholder="ex 13:00" required>
  </div>
      </div>
           <div class="col-lg-6">
  <div class="form-group" >
    <label for="ankoms">Ankomst</label>
    <input type="text" class="form-control" name="ankomst" placeholder="ex 18:30" required>
  </div>
      </div>
                  
               </div>
               <div class="row">
                  
           <div class="col-lg-4">
  <div class="form-group" >
    <label for="pris">Pris</label>
    <input type="text" class="form-control" name="pris" placeholder="ex 599" required>
  </div>
      </div>
                 <div class="col-lg-4">
  <div class="form-group" >
    <label for="platser">Platser</label>
    <input type="text" class="form-control" name="platser" placeholder="ex 45" required>
  </div>
      </div>
                 <div class="col-lg-4">
  <div class="form-group" >
    <label for="chaufforid">Chaufförid</label>
    <input type="text" class="form-control" name="chaufforid" placeholder="ex 551122-3344" value="Ej bestämt" required>
  </div>
      </div>
          </div>
              
    </div>          
      
      </div>
         <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal">Avbryt</button>
        <input type="submit" class="btn btn-primary" value="Spara" />
      </div>
        </form>
    </div>
  </div>
</div>
   
